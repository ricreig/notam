{
  "name": "notam-api",
  "version": "0.1.0",
  "description": "NOTAM operational dashboard API",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
